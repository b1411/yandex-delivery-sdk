{
    "compilerOptions": {
        "outDir": "./dist",
        "declaration": true,
        "declarationDir": "./dist/types",
        "sourceMap": true,
        "module": "ES6",
        "target": "ES6",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
}
